<p>users/index</p>
<!--左部分-->
<h2>User info</h2>
<%= render 'user', user: current_user %>

<h2>New book</h2>


<!--右部分-->
<h1>ユーザー一覧</h1>

<% @users.each do |user| %>
  <div class="user">
    <%= image_tag user.get_profile_image(80,80) %>
    <h2><%= user.name %></h2>
    <p><%= user.introduction %></p>
    <%= link_to "詳細", user_path(user) %>
  </div>
<% end %>