  <h1>User info</h1>
  <%= image_tag user.get_profile_image(100,100) %>

  <div>
    <p>name</p>
    <p><%= user.name %></p>
  </div>

  <div>
    <p>introduction</p>
    <p><%= user.introduction %></p>
  </div>

  <div>
    <% if user.id == current_user.id %>
      <%= link_to "edit", edit_user_path %>
    <% end %>
  </div>
  
  